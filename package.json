{
  "name": "vue-step-wizard",
  "version": "0.2.0",
  "private": false,
  "description": "A simple vuejs step/form wizard plugin. modified significantly from the original to work for th prototype.",
  "author": {
    "name": "Tushar Gugnani",
    "email": "tushar@5balloons.info"
  },
  "contributors": {
    "name": "m. milano",
    "email": "mmilano+git@gmail.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-library": "vue-cli-service build --target lib --name vue-step-wizard ./src/install.js"
  },
  "main": "./dist/vue-step-wizard.common.js",
  "files": [
    "src/components/wizard/*",
    "src/store/modules/wizard.js",
    "src/assets/scss/components/*"
  ],
  "dependencies": {
    "core-js": "^3.12.1",
    "vue": "^2.6.12",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.4",
    "babel-eslint": "^10.0.1",
    "bootstrap": "^4.5.2",
    "eslint": "^7.7.0",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.32.12",
    "sass-loader": "^9.0.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 2%",
    "last 2 versions"
  ],
  "bugs": "https://github.com/thoriumdata/vue-step-wizard/issues",
  "keywords": [
    "vue form wizard",
    "vue stepper",
    "vue multi step form",
    "step component vue"
  ],
  "license": "MIT",
  "repository": "https://github.com/thoriumdata/vue-step-wizard"
}
